    app:startDestination="@+id/navigation_home">

    <fragment android:id="@+id/navigation_home"
        android:name="com.thehealingradio.TheHealingRadio.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home"/>

    <fragment android:id="@+id/navigation_blog"
        android:name="com.thehealingradio.TheHealingRadio.ui.blog.BlogFragment"
